@model IEnumerable<Fortigate_Gui.Models.VpnSetting>

@{
    ViewData["Title"] = "VPN Settings";
}

<div class="row">
    <div class="col-3">
<p>
            <a onclick="showInPopup('@Url.Action("AddOrEdit","VpnSetting",null,Context.Request.Scheme)','Add VPN Setting')" class="btn btn-outline-success">New VPN Setting</a>
</p>
    </div>
    <div class="col-9">
        <h1><i class="fas fa-sliders-h"></i> VPN Settings</h1>
    </div>
</div>

<div id="view-all">
    @await Html.PartialAsync("_ViewAll", Model)
</div>
