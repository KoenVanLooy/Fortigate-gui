@model IEnumerable<Fortigate_Gui.Models.Ip4Policy>

@{
    ViewData["Title"] = "IPv4 Policy";
}

<div class="row">
    <div class="col-4">
        <p>
            <a onclick="showInPopup('@Url.Action("Create","Ip4Policy",null,Context.Request.Scheme)','Add Ip4Policy')" class="btn btn-outline-danger">New Ip4Policy</a>
        </p>
    </div>
    <div class="col-8">
        <h1><i class="fas fa-code"></i> Ip4Policy</h1>
    </div>
</div>

<div id="view-all">
    @await Html.PartialAsync("_ViewAll", Model)
</div>

