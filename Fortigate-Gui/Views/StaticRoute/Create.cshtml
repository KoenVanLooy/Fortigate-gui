@model Fortigate_Gui.Models.StaticRoute

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>StaticRoute</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="InterfaceID" class="control-label"></label>
                <select asp-for="InterfaceID" class ="form-control" asp-items="ViewBag.InterfaceID"></select>
            </div>
            <div class="form-group">
                <label asp-for="DestinationSubnet" class="control-label"></label>
                <input asp-for="DestinationSubnet" class="form-control" />
                <span asp-validation-for="DestinationSubnet" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Gateway" class="control-label"></label>
                <input asp-for="Gateway" class="form-control" />
                <span asp-validation-for="Gateway" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-outline-danger" />
            </div>
        </form>
    </div>
</div>

