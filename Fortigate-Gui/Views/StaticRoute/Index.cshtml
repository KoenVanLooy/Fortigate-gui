@model IEnumerable<Fortigate_Gui.Models.StaticRoute>

@{
    ViewData["Title"] = "StaticRoutes";
}

<div class="row">
    <div class="col-4">
        <p>
            <a onclick="showInPopup('@Url.Action("Create","StaticRoute",null,Context.Request.Scheme)','Add Static Route')" class="btn btn-outline-danger">New Static Route</a>
        </p>
    </div>
    <div class="col-8">
        <h1><i class="fas fa-route"></i> StaticRoutes</h1>
    </div>
</div>

<div id="view-all">
    @await Html.PartialAsync("_ViewAll", Model)
</div>