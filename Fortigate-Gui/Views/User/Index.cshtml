@model IEnumerable<Fortigate_Gui.Models.FortiUser>

@{
    ViewData["Title"] = "Index";
}

<h1 class="text-center">Users</h1>

<p>
    <a onclick="showInPopup('@Url.Action("AddOrEdit","User",null,Context.Request.Scheme)','New User')" class="btn btn-success">New User</a>
</p>

<div id="view-all">
    @await Html.PartialAsync("_ViewAll", Model)
</div>

