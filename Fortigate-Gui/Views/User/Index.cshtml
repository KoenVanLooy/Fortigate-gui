@model IEnumerable<Fortigate_Gui.Models.FortiUser>

@{
    ViewData["Title"] = "Users";
}

<div class="row">
    <div class="col-4">
        <p>
            <a onclick="showInPopup('@Url.Action("AddOrEdit","User",null,Context.Request.Scheme)','Add User')" class="btn btn-outline-danger">New User</a>
        </p>
    </div>
    <div class="col-8">
        <h1><i class="fas fa-user"></i> Users</h1>
    </div>
</div>

<div id="view-all">
    @await Html.PartialAsync("_ViewAll", Model)
</div>