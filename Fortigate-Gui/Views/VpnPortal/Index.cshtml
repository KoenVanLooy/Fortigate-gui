@model IEnumerable<Fortigate_Gui.Models.VpnPortal>

@{
    ViewData["Title"] = "VPN Portals";
}

<div class="row">
    <div class="col-3">
        <p>
            <a onclick="showInPopup('@Url.Action("AddOrEdit","VpnPortal",null,Context.Request.Scheme)','Add VPN Portal')" class="btn btn-outline-success">New VPN Portal</a>
        </p>
    </div>
    <div class="col-9">
        <h1><i class="fas fa-ethernet"></i> VPN Portals</h1>
    </div>
</div>

<div id="view-all">
    @await Html.PartialAsync("_ViewAll", Model)
</div>
