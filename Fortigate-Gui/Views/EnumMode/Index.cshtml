@model IEnumerable<Fortigate_Gui.Models.EnumMode>

@{
    ViewData["Title"] = "Mode";
}

<div class="row">
    <div class="col-4">
        <p>
            <a onclick="showInPopup('@Url.Action("Create","EnumMode",null,Context.Request.Scheme)','Add Mode')" class="btn btn-outline-danger">New Mode</a>
        </p>
    </div>
    <div class="col-8">
        <h1><i class="fas fa-compress"></i> Mode</h1>
    </div>
</div>

<div id="view-all">
    @await Html.PartialAsync("_ViewAll", Model)
</div>