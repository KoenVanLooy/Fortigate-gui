@model IEnumerable<Fortigate_Gui.Models.EnumAcces>

@{
    ViewData["Title"] = "Acces";
}

<div class="row">
    <div class="col-4">
        <p>
            <a onclick="showInPopup('@Url.Action("Create","EnumAcces",null,Context.Request.Scheme)','Add Acces')" class="btn btn-outline-danger">New Acces</a>
        </p>
    </div>
    <div class="col-8">
        <h1><i class="fas fa-universal-access"></i> Acces</h1>
    </div>
</div>

<div id="view-all">
    @await Html.PartialAsync("_ViewAll", Model)
</div>