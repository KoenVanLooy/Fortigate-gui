@model IEnumerable<Fortigate_Gui.Models.Interface>

@{
    ViewData["Title"] = "Index";
}

<div class="row">
    <div class="col-4">
        <p>
            <a onclick="showInPopup('@Url.Action("Create","Interface",null,Context.Request.Scheme)','Create Interface')" class="btn btn-outline-danger">New Interface</a>
        </p>
    </div>
    <div class="col-8">
        <h1><i class="fas fa-info-circle"></i> Interfaces</h1>
    </div>
</div>

<div id="view-all">
    @await Html.PartialAsync("_ViewAll",Model)
</div>
