@model IEnumerable<Fortigate_Gui.Models.EnumPhysical>

@{
    ViewData["Title"] = "Physical";
}

<div class="row">
    <div class="col-4">
        <p>
            <a onclick="showInPopup('@Url.Action("Create","EnumPhysical",null,Context.Request.Scheme)','Add Physical')" class="btn btn-outline-danger">New Physical</a>
        </p>
    </div>
    <div class="col-8">
        <h1><i class="fas fa-dumbbell"></i> Physical</h1>
    </div>
</div>

<div id="view-all">
    @await Html.PartialAsync("_ViewAll", Model)
</div>